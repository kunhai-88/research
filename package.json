    "autoprefixer": "9.5.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "better-npm-run": "^0.1.1",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
